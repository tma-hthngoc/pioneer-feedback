<% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value.html_safe %>
      <span id="renew_feedback">Click here to send another
        <%= link_to root_url, "data-no-turbolink" => true do %>
            <b>feedback</b>
        <% end %>
      </span>
    </div>
<% end %>
